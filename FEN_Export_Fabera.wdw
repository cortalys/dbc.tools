  -
    name : BTN_Exporter
    identifier : 0x290e284727284303
    internal_properties : 
    properties :
     x : 6
     y : 58
     width : 99
     height : 24
     accelerator_key : 65535
     tab_order : 0
    style : {}
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
        type : 14
      -
        code : |1+
         // Data control
         sRepertoireExport		est une chaîne	= ComplèteRep(SAI_Repertoire_Export)
         SI sRepertoireExport = "" OU PAS fRepExiste(sRepertoireExport) ALORS
         	Erreur("Le répertoire d'export est obligatoire")
         	RepriseSaisie(SAI_Repertoire_Export)
         FIN
         sRepertoireExport=completerep(sRepertoireExport+DateDuJour+HeureSys)
         fRepCrée(sRepertoireExport)
         
         Sablier(Vrai)
         
         xlsFileName	est une chaîne	= fConstruitChemin(sRepertoireExport,"export_clipper_fabera",".xlsx")
         logFabera	est une CL_Log("FABERA",fConstruitChemin(sRepertoireExport,"logclipper_fabera",".log"))
         logFabera.LogInfo("Répertoire des documents "+sRepDocuments)
         logFabera.LogInfo("Répertoire des images "+sRepImages)
         
         // Create XL document
         xlsFic		est un xlsDocument
         xlsAjouteFeuille(xlsFic,"Export Clipper")
         xlsFic..Feuille					= 1
         // Set titles
         POUR TOUT ÉLÉMENT col,titre DE tabEntete
         	xlsFic..Ligne[1]..Cellule[col]	= titre
         FIN
         
         i					est un entier		= 2
         j					est un entier		= 2
         nCpt				est un entier		= 0
         sFiltreCodePiece	est une chaîne		= SAI_Piècecyclée
         sFiltreRepere		est une chaîne		= SAI_Repère
         dFiltreDebut		est une date		= SAI_DateDebut
         dFiltreFin			est une date		= SAI_DateFin
         sdArt				est une Requête SQL	=
         [
         	SELECT ART.* FROM ART<§cs:3§>
         	LEFT OUTER JOIN ENGAPI ON ENGAPI.EN_PIECE = ART.GPCOPIECE AND ENGAPI.MODELE=art.MODELEDT AND ENGAPI.RANG = '001'<§cs:3§>
         	LEFT OUTER JOIN HISTOFIC ON HISTOFIC.ABREVFAM = 'AR' AND HISTOFIC.LIBELLE = ART.GPCOPIECE<§cs:3§>
         	WHERE ART.GPCOPIECE LIKE {codePiece} AND ENGAPI.REPERE LIKE {repere} AND HISTOFIC.DAT >= {dateDebut} AND HISTOFIC.DAT <= {dateFin}<§cs:3§>
         ]
         sdArt.codePiece	= sFiltreCodePiece<>"" ? sFiltreCodePiece+"%" sinon null
         sdArt.repere	= sFiltreRepere<>"" ? sFiltreRepere+"%" SINON Null
         sdArt.dateDebut	= datevalide(dFiltreDebut) ? dFiltreDebut sinon null
         sdArt.dateFin	= DateValide(dFiltreFin) ? dFiltreFin SINON Null
         // Search products
         SI PAS HExécuteRequêteSQL(sdArt) ALORS
         	logFabera.LogError("Erreur lors de l'exécution de la requête : "+Remplace(sdArt.CodeSQL,RC," "))
         	logFabera.LogError("Détail de l'erreur : "+HErreurInfo())
         	Erreur(HErreurInfo())
         	RETOUR
         FIN
         logFabera.LogInfo("Requête pièces cyclées "+remplace(sdArt.CodeSQLExécuté,rc," "))
         
         // Iterate products
         HLitPremier(sdArt)
         Message(CBM_NbEnr,HNbEnr(sdArt)+" pièce(s)")
         logFabera.LogInfo(HNbEnr(sdArt)+" pièce(s) cyclée(s) trouvée(s)")
         MultitâcheRepeint(FEN_Export_Fabera)
         TANTQUE PAS HEnDehors(sdArt)
         	logFabera.LogInfo(sansespace(sdArt.GPCOPIECE) +" -> Début")
         	// Common rows
         	HLitRecherche(CLIENT,"COCLI",sdArt.COCLI)
         	SI HTrouve(CLIENT) ALORS xlsFic..Ligne[i]..Cellule[tabEntete[sCocli]] = SansEspace(CLIENT.COCLI)
         	xlsFic..Ligne[i]..Cellule[tabEntete[sRepere]]		= ""
         	xlsFic..Ligne[i]..Cellule[tabEntete[sRef]]			= SansEspace(sdArt.GPCOPIECE)
         	xlsFic..Ligne[i]..Cellule[tabEntete[sDes]]			= SansEspace(sdArt.DESA1)
         	HLitRecherchePremier(HISTOFIC,ABREVLIB,["AR",sdArt.GPCOPIECE])
         	SI HTrouve(HISTOFIC) ALORS xlsFic..Ligne[i]..Cellule[tabEntete[sDateCreation]] = DateVersChaîne(HISTOFIC.DAT,"AAAA-MM-JJ") SINON xlsFic..Ligne[i]..Cellule[tabEntete[sDateCreation]]=""
         	nQteEco est un numérique = sdArt.QTEECO >0 ? sdArt.QTEECO SINON 1
         	xlsFic..Ligne[i]..Cellule[tabEntete[sQteEco]]	= nQteEco
         	xlsFic..Ligne[i]..Cellule[tabEntete[s2DClient]]	= ""
         	xlsFic..Ligne[i]..Cellule[tabEntete[s3DClient]]	= ""
         	xlsFic..Ligne[i]..Cellule[tabEntete[s2DPhase]]	= ""
         	xlsFic..Ligne[i]..Cellule[tabEntete[s3DPhase]]	= ""
         
         	SI HLitRecherchePremier(ENGAPI,EPCLE,[sdArt.GPCOPIECE,sdArt.MODELEDT,Complète("001",Dimension(ENGAPI.RANG))]) ALORS
         		// Search product files (2D & 3D drawings)
         		sDrawing2D est une chaîne = SansEspace(ENGAPI.NomFichier)
         		logFabera.LogInfo(SansEspace(sdArt.GPCOPIECE)+" Plan 2D client dans l'oeil : "+sDrawing2D)
         		si sDrawing2D="" alors
         			sdProductDrawing2D est une Requête SQL	=
         			[
         				SELECT DocumentsENGAPI.NomFichier AS FichierDocuments, NORME.LIBELLE, NORME.REPDOC AS FichierNorme<§cs:3§>
         				FROM DocumentsENGAPI<§cs:3§>
         				LEFT OUTER JOIN NORME ON NORME.N0CLEUNIK = DocumentsENGAPI.N0CLEUNIK<§cs:3§>
         				WHERE DocumentsENGAPI.EPCLEUNIK = {epCleunik} AND NORME.LIBELLE = 'PLAN CLIENT'<§cs:3§>
         			]
         			sdProductDrawing2D.epCleunik = engapi.EPCLEUNIK
         			si HExécuteRequêteSQL(sdProductDrawing2D) alors
         				HLitPremier(sdProductDrawing2D)
         				SI PAS HEnDehors(sdProductDrawing2D) ALORS
         					sDrawing2D=SansEspace(sdProductDrawing2D.FichierDocuments)
         					si sDrawing2D= "" alors sDrawing2D=SansEspace(sdProductDrawing2D.FichierNorme)
         					logFabera.LogInfo(SansEspace(sdArt.GPCOPIECE)+" Plan 2D client dans la GED : "+sDrawing2D)
         				FIN
         			FIN
         			HLibèreRequête(sdProductDrawing2D)
         		FIN
         		SI sDrawing2D<>"" ALORS
         			// 2d drawing copy
         			SI EnModeTest() ALORS sDrawing2D = Remplace(sDrawing2D,"P:\CLIENT","<RépertoireDocuments>",SansCasse)
         			sDrawing2D	= Remplace(sDrawing2D,"<RépertoireDocuments>",sRepDocuments)
         			sDrawing2D	= Remplace(sDrawing2D,"<RépertoireImages>",sRepImages)
         			sDrawing2D	= Remplace(sDrawing2D,"\\","\")
         			SI pas fFichierExiste(sDrawing2D) ALORS
         				logFabera.LogWarning(SansEspace(sdArt.GPCOPIECE)+" Plan 2D client introuvable : "+sDrawing2D)
         			sinon
         				sFichier2D est une chaîne = fConstruitChemin(sRepertoireExport+"2DClient",fExtraitChemin(sDrawing2D,fFichier+fExtension))
         				fCopieFichier(sDrawing2D,sFichier2D)
         				logFabera.LogInfo(SansEspace(sdArt.GPCOPIECE)+" Copie du fichier vers : "+fExtraitChemin(sFichier2D,fFichier+fExtension))
         				xlsFic..Ligne[i]..Cellule[tabEntete[s2DClient]] = fExtraitChemin(sFichier2D,fFichier+fExtension)
         				// 3d drawing steps search and copy
         				sDrawingFolder	est une chaine	= completerep(fExtraitChemin(sDrawing2D,fDisque+fRépertoire))
         				sSteps			est une chaine	= fListeFichier(sDrawingFolder+ "*.step",frNonRécursif)
         				logFabera.LogInfo(SansEspace(sdArt.GPCOPIECE)+" Liste des plans 3D STEP : "+Remplace(sSteps,RC,","))
         				sColPlan3D est une chaîne
         				POUR TOUTE CHAÎNE sStep DE sSteps SÉPARÉE PAR RC
         					sFichier3D est une chaîne = fConstruitChemin(sRepertoireExport+"3DClient",fExtraitChemin(sStep,fFichier),fExtraitChemin(sStep,fExtension))
         					fCopieFichier(sStep,sFichier3D)
         					logFabera.LogInfo(SansEspace(sdArt.GPCOPIECE)+" Copie du plan 3D client : "+fExtraitChemin(sFichier3D,fFichier+fExtension))
         					SI sColPlan3D<>"" ALORS sColPlan3D+="|"
         					sColPlan3D+=fExtraitChemin(sFichier3D,fFichier+fExtension)
         				FIN
         				SI sColPlan3D<>"" ALORS xlsFic..Ligne[i]..Cellule[tabEntete[s3DClient]] = sColPlan3D
         				// 3d drawing SW search and copy
         				sSldprts		est une chaine	= fListeFichier(sDrawingFolder+ "*.sldprt",frNonRécursif)
         				logFabera.LogInfo(SansEspace(sdArt.GPCOPIECE)+" Liste des plans 3D SLDPRT : "+remplace(sSldprts,rc,","))
         				POUR TOUTE CHAÎNE sSldprt DE sSldprts SÉPARÉE PAR RC
         					sFichier3D est une chaîne = fConstruitChemin(sRepertoireExport+"3DPhase",fExtraitChemin(sSldprt,fFichier),fExtraitChemin(sSldprt,fExtension))
         					fCopieFichier(sSldprt,sFichier3D)
         					logFabera.LogInfo(SansEspace(sdArt.GPCOPIECE)+" Copie du plan 3D phase : "+fExtraitChemin(sFichier3D,fFichier+fExtension))
         				FIN
         			FIN
         		FIN
         	FIN
         
         	// Manufacturing
         	sdGapiece est une requete sql =
         	[
         		SELECT * FROM GAPIECE WHERE EPCLEUNIK = {epcleunik} ORDER BY PHASE<§cs:3§>
         	]
         	sdGapiece.epcleunik=ENGAPI.EPCLEUNIK
         	SI HExécuteRequêteSQL(sdGapiece) ALORS
         		hlitpremier(sdGapiece)
         		TANTQUE pas hendehors(sdGapiece)
         			SI SansEspace(sdGapiece.COFRAIS)<>"NOMEN" ALORS
         				xlsFic..Ligne[j]..Cellule[tabEntete[sCocli]]		= xlsFic..Ligne[i]..Cellule[tabEntete[sCocli]]
         				xlsFic..Ligne[j]..Cellule[tabEntete[sRepere]]		= xlsFic..Ligne[i]..Cellule[tabEntete[sRepere]]
         				xlsFic..Ligne[j]..Cellule[tabEntete[sRef]]			= xlsFic..Ligne[i]..Cellule[tabEntete[sRef]]
         				xlsFic..Ligne[j]..Cellule[tabEntete[sDes]]			= xlsFic..Ligne[i]..Cellule[tabEntete[sDes]]
         				xlsFic..Ligne[j]..Cellule[tabEntete[sDateCreation]]	= xlsFic..Ligne[i]..Cellule[tabEntete[sDateCreation]]
         				xlsFic..Ligne[j]..Cellule[tabEntete[sQteEco]]		= xlsFic..Ligne[i]..Cellule[tabEntete[sQteEco]]
         				xlsFic..Ligne[j]..Cellule[tabEntete[s2DClient]]		= xlsFic..Ligne[i]..Cellule[tabEntete[s2DClient]]
         				xlsFic..Ligne[j]..Cellule[tabEntete[s3DClient]]		= xlsFic..Ligne[i]..Cellule[tabEntete[s3DClient]]
         				xlsFic..Ligne[j]..Cellule[tabEntete[sCDF]]			= SansEspace(sdGapiece.COFRAIS)+" "+SansEspace(sdGapiece.GA_DES)
         				xlsFic..Ligne[j]..Cellule[tabEntete[sType]]			= "VA"
         				xlsFic..Ligne[j]..Cellule[tabEntete[sQteNom]]		= 1
         				xlsFic..Ligne[j]..Cellule[tabEntete[sTpsPrepa]]		= sdGapiece.GA_PREP
         				xlsFic..Ligne[j]..Cellule[tabEntete[sTpsUnit]]		= sdGapiece.GA_NBH
         				xlsFic..Ligne[j]..Cellule[tabEntete[sEffPrepa]]		= sdGapiece.NBPERSTP
         				xlsFic..Ligne[j]..Cellule[tabEntete[sEffUnit]]		= sdGapiece.NBPERSTU
         				xlsFic..Ligne[j]..Cellule[tabEntete[sCout]]			= sdGapiece.GA_NBH+sdGapiece.GA_PREP = 0 ? 0 SINON (sdGapiece.GA_NBH*nQteEco+sdGapiece.GA_PREP)/nQteEco * (sdGapiece.GA_WPRIX/(sdGapiece.GA_NBH+sdGapiece.GA_PREP))
         				xlsFic..Ligne[j]..Cellule[tabEntete[sNumOp]]		= sdGapiece.GPCLEUNIK
         				xlsFic..Ligne[j]..Cellule[tabEntete[sNumOpSuiv]]	= 0
         				logFabera.LogInfo(SansEspace(sdArt.GPCOPIECE)+" Ajout de la phase de gamme "+sdGapiece.PHASE+" - "+sansespace(sdGapiece.GA_DES))
         				// Search phase drawing
         				sdPhaseDrawing2D est une Requête SQL	=
         				[
         					SELECT NORME.REPDOC AS FichierDocuments <§cs:3§>
         					FROM NORME <§cs:3§>
         					WHERE CONORME = (<§cs:3§>
         					SELECT NORME.CONORME<§cs:3§>
         					FROM DocumentsGAPIECE<§cs:3§>
         					JOIN NORME ON NORME.N0CLEUNIK = DocumentsGAPIECE.N0CLEUNIK //AND (NORME.LIBELLE = 'PLAN DE PHASE' OR NORME.LIBELLE LIKE 'CDC%')<§cs:3§>
         					WHERE DocumentsGAPIECE.GPCLEUNIK={gpCleunik}) AND FLAG = 'O'<§cs:3§>
         				]
         
         				sdPhaseDrawing2D.gpCleunik=sdGapiece.GPCLEUNIK
         				sPhaseDrawing2D est une chaîne
         				SI HExécuteRequêteSQL(sdPhaseDrawing2D) ALORS
         					SI PAS HEnDehors(sdPhaseDrawing2D) ALORS
         						sPhaseDrawing2D=SansEspace(sdPhaseDrawing2D.FichierDocuments)
         						logFabera.LogInfo(SansEspace(sdArt.GPCOPIECE)+" Plan 2D Phase dans la GED : "+sPhaseDrawing2D)
         						SI EnModeTest() ALORS sPhaseDrawing2D = Remplace(sPhaseDrawing2D,"P:\CLIENT","<RépertoireDocuments>",SansCasse)
         						sPhaseDrawing2D	= Remplace(sPhaseDrawing2D,"<RépertoireDocuments>",sRepDocuments)
         						sPhaseDrawing2D	= Remplace(sPhaseDrawing2D,"<RépertoireImages>",sRepImages)
         						sPhaseDrawing2D	= Remplace(sPhaseDrawing2D,"\\","\")
         						SI pas fFichierExiste(sPhaseDrawing2D) ALORS
         							logFabera.LogWarning(SansEspace(sdArt.GPCOPIECE)+" Plan 2D phase introuvable : "+sPhaseDrawing2D)
         						sinon
         							sFichierPhase2D est une chaîne = fConstruitChemin(sRepertoireExport+"2DPhase",fExtraitChemin(sPhaseDrawing2D,fFichier+fExtension))
         							fCopieFichier(sPhaseDrawing2D,sFichierPhase2D)
         							logFabera.LogInfo(SansEspace(sdArt.GPCOPIECE)+" Copie du plan 2D phase : "+fExtraitChemin(sFichierPhase2D,fFichier+fExtension))
         							xlsFic..Ligne[j]..Cellule[tabEntete[s2DPhase]]	= fExtraitChemin(sFichierPhase2D,fFichier+fExtension)
         							sFichierPhase3D		est une chaine	= ExtraitChaîne(fExtraitChemin(sFichierPhase2D,fFichier),1," ")
         							sPrefixe			est une chaine	= ExtraitChaîne(sFichierPhase3D,1,"-")
         							sFichierPhase3D									= sPrefixe+".SLDPRT_"+sFichierPhase3D+".step"
         							xlsFic..Ligne[j]..Cellule[tabEntete[s3DPhase]]	= sFichierPhase3D
         						FIN
         					FIN
         				FIN
         				HLibèreRequête(sdPhaseDrawing2D)
         				j = j+1
         			FIN
         			HLitSuivant(sdGapiece)
         		FIN
         	FIN
         
         	// Bill of material
         	HFiltre(NOMENPC,CLENC,[ENGAPI.GPCOPIECE,ENGAPI.MODELE,ENGAPI.RANG,hValMin],[ENGAPI.GPCOPIECE,ENGAPI.MODELE,ENGAPI.RANG,hValMax])
         	HLitPremier(NOMENPC,CLENC)
         	TANTQUE PAS HEnDehors(NOMENPC)
         		xlsFic..Ligne[j]..Cellule[tabEntete[sCocli]]		= xlsFic..Ligne[i]..Cellule[tabEntete[sCocli]]
         		xlsFic..Ligne[j]..Cellule[tabEntete[sRepere]]		= xlsFic..Ligne[i]..Cellule[tabEntete[sRepere]]
         		xlsFic..Ligne[j]..Cellule[tabEntete[sRef]]			= xlsFic..Ligne[i]..Cellule[tabEntete[sRef]]
         		xlsFic..Ligne[j]..Cellule[tabEntete[sDes]]			= xlsFic..Ligne[i]..Cellule[tabEntete[sDes]]
         		xlsFic..Ligne[j]..Cellule[tabEntete[sDateCreation]]	= xlsFic..Ligne[i]..Cellule[tabEntete[sDateCreation]]
         		xlsFic..Ligne[j]..Cellule[tabEntete[sQteEco]]		= xlsFic..Ligne[i]..Cellule[tabEntete[sQteEco]]
         		xlsFic..Ligne[j]..Cellule[tabEntete[s2DClient]]		= xlsFic..Ligne[i]..Cellule[tabEntete[s2DClient]]
         		xlsFic..Ligne[j]..Cellule[tabEntete[s3DClient]]		= xlsFic..Ligne[i]..Cellule[tabEntete[s3DClient]]
         		xlsFic..Ligne[j]..Cellule[tabEntete[s2DPhase]]		= xlsFic..Ligne[i]..Cellule[tabEntete[s2DPhase]]
         		xlsFic..Ligne[j]..Cellule[tabEntete[s3DPhase]]		= xlsFic..Ligne[i]..Cellule[tabEntete[s3DPhase]]
         		xlsFic..Ligne[j]..Cellule[tabEntete[sCDF]]			= SansEspace(NOMENPC.NPCE)
         		xlsFic..Ligne[j]..Cellule[tabEntete[sType]]			= Gauche(NOMENPC.NPCE,4)="STR/" ? "ST" SINON "AC"
         		xlsFic..Ligne[j]..Cellule[tabEntete[sQteNom]]		= NOMENPC.COEF
         		xlsFic..Ligne[j]..Cellule[tabEntete[sTpsPrepa]]		= 0
         		xlsFic..Ligne[j]..Cellule[tabEntete[sTpsUnit]]		= 0
         		xlsFic..Ligne[j]..Cellule[tabEntete[sEffPrepa]]		= 0
         		xlsFic..Ligne[j]..Cellule[tabEntete[sEffUnit]]		= 0
         		xlsFic..Ligne[j]..Cellule[tabEntete[sCout]]			= NOMENPC.COEF*NOMENPC.PRIXART*NOMENPC.CoefPrix
         		xlsFic..Ligne[j]..Cellule[tabEntete[sNumOp]]		= 0
         		xlsFic..Ligne[j]..Cellule[tabEntete[sNumOpSuiv]]	= NOMENPC.GPCLEUNIK
         		j													= j+1
         		logFabera.LogInfo(SansEspace(sdArt.GPCOPIECE)+" Ajout de la phase de nomenclature "+SansEspace(NOMENPC.NPCE))
         		HLitSuivant(NOMENPC,CLENC)
         	FIN
         	HDésactiveFiltre(NOMENPC)
         
         	nCpt++
         	Message(CBM_Enr,"Pièce "+nCpt+" : "+SansEspace(sdArt.GPCOPIECE))
         	logFabera.LogInfo(SansEspace(sdArt.GPCOPIECE) +" -> Fin")
         	MultitâcheRepeint(FEN_Export_Fabera)
         	HLitSuivant(sdArt)
         FIN
         HLibèreRequête(sdArt)
         
         // Save XL document
         xlsFic.Sauve(xlsFileName)
         xlsFerme(xlsFic)
         
